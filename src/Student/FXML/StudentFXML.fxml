         </children>
      </HBox>
      <VBox alignment="TOP_RIGHT" layoutX="-11.0" layoutY="69.0" prefHeight="379.0" prefWidth="115.0" style="-fx-background-color: #29B6F6; -fx-background-radius: 5;">
         <children>
            <JFXButton fx:id="btnStudentPersonalInfo" onMouseClicked="#loadStudentPersonalInfo" prefHeight="60.0" prefWidth="104.0" text="مشخصات فردی">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="btnStudentWeeklyPrograms" onMouseClicked="#loadStudentWeeklyProgram" prefHeight="60.0" prefWidth="104.0" text="برنامه هفتگی">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </JFXButton>
         </children>
      </VBox>
      <AnchorPane fx:id="StudentMainAnchorPane" layoutX="117.0" layoutY="69.0" prefHeight="379.0" prefWidth="618.0" style="-fx-background-color: B2EBF2; -fx-background-radius: 5;">
