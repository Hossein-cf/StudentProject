<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="749.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Student.StudentController">
   <children>
      <HBox alignment="CENTER_RIGHT" prefHeight="41.0" prefWidth="749.0" style="-fx-background-color: #006DB3;">
         <children>
            <JFXButton fx:id="btnBack" onMouseClicked="#back" prefHeight="39.0" prefWidth="59.0" text="بازگشت" />
